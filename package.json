{
	"name": "mind-mapper",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "FIRESTORE_EMULATOR_HOST=localhost:8080 FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 vite dev --host 0.0.0.0",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"emulators": "firebase emulators:start --only firestore,auth --import=./.firebase_emulators --export-on-exit=./.firebase_emulators",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@iconify-json/mdi": "^1.1.67",
		"@iconify/tailwind": "^1.1.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.7",
		"@types/firebase": "^3.2.1",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.12.10",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.39",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@anthropic-ai/sdk": "^0.24.3",
		"@firebase/auth": "^1.7.4",
		"@magidoc/plugin-svelte-marked": "^6.2.0",
		"@types/marked": "^5.0.2",
		"firebase": "^10.12.2",
		"firebase-admin": "^12.2.0",
		"marked": "^15.0.6",
		"mathjs": "^14.0.1",
		"svelte-highlight": "^7.8.2",
		"svelte-markdown": "^0.4.1",
		"swiper": "^11.1.15",
		"theme-change": "^2.5.0",
		"voyageai": "^0.0.3-1"
	}
}
